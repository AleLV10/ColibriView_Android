<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Inicio">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="82dp"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/image"
                android:layout_width="match_parent"
                android:layout_height="198dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="55dp"
                >

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/tipos"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
                <Button
                    android:id="@+id/ver"
                    android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="20dp"
                    android:backgroundTint="#7858F2"
                    android:text="VER RESULTADOS"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/carrd_test"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/image"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:clickable="true"
                android:focusable="true">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/common_padding_default"
                    android:paddingTop="@dimen/common_padding_default"
                    android:paddingEnd="@dimen/common_padding_default"
                    android:paddingBottom="@dimen/common_padding_default"
                    android:background="#EDF2FA"
                    >

                    <ImageView
                        android:id="@+id/imgCard2"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/seis"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/title_tx2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/nomtest"
                        android:textSize="@dimen/tamaño_letra_sub_subtitulo"
                        android:textStyle="bold"
                        android:textColor="@color/color"
                        app:layout_constraintStart_toEndOf="@id/imgCard2"
                        app:layout_constraintTop_toTopOf="@id/imgCard2" />

                    <TextView
                        android:id="@+id/describe2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/des_ishihara"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="@id/title_tx2"
                        app:layout_constraintTop_toBottomOf="@id/title_tx2"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginBottom="10dp"

                        />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>



            <com.google.android.material.card.MaterialCardView
                android:id="@+id/carrd2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/carrd_test"
                android:clickable="true"
                android:focusable="true">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/common_padding_default"
                    android:paddingTop="@dimen/common_padding_default"
                    android:paddingEnd="@dimen/common_padding_default"
                    android:paddingBottom="@dimen/common_padding_default"
                    android:background="#EDF2FA"
                    >

                    <ImageView
                        android:id="@+id/imgCard"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/iconpd"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/title_tx1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/nomtest_PD"
                        android:textSize="@dimen/tamaño_letra_sub_subtitulo"
                        android:textStyle="bold"
                        android:textColor="@color/color"
                        app:layout_constraintStart_toEndOf="@id/imgCard"
                        app:layout_constraintTop_toTopOf="@id/imgCard" />

                    <TextView
                        android:id="@+id/describe"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/des_PD"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="@id/title_tx1"
                        app:layout_constraintTop_toBottomOf="@id/title_tx1"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginBottom="10dp"

                        />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>



            <com.google.android.material.card.MaterialCardView
                android:id="@+id/carrd3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/carrd2"
                android:clickable="true"
                android:focusable="true">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/common_padding_default"
                    android:paddingTop="@dimen/common_padding_default"
                    android:paddingEnd="@dimen/common_padding_default"
                    android:paddingBottom="@dimen/common_padding_default"
                    android:background="#EDF2FA"
                    >

                    <ImageView
                        android:id="@+id/imgCard_2"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/icontr"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/title_tx1_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/nomtest_Tr"
                        android:textSize="@dimen/tamaño_letra_sub_subtitulo"
                        android:textStyle="bold"
                        android:textColor="@color/color"
                        app:layout_constraintStart_toEndOf="@id/imgCard_2"
                        app:layout_constraintTop_toTopOf="@id/imgCard_2" />

                    <TextView
                        android:id="@+id/describe_2"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/des_TR"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="@id/title_tx1_2"
                        app:layout_constraintTop_toBottomOf="@id/title_tx1_2"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginBottom="10dp"
                        />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>




            <com.google.android.material.card.MaterialCardView
                android:id="@+id/carrd4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/carrd3"
                android:clickable="true"
                android:focusable="true">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="@dimen/common_padding_default"
                    android:paddingTop="@dimen/common_padding_default"
                    android:paddingEnd="@dimen/common_padding_default"
                    android:paddingBottom="@dimen/common_padding_default"
                    android:background="#EDF2FA"
                    >

                    <ImageView
                        android:id="@+id/imgCard_3"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/icontl"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/title_tx1_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/nomtest_Tl"
                        android:textSize="@dimen/tamaño_letra_sub_subtitulo"
                        android:textStyle="bold"
                        android:textColor="@color/color"
                        app:layout_constraintStart_toEndOf="@id/imgCard_3"
                        app:layout_constraintTop_toTopOf="@id/imgCard_3" />

                    <TextView
                        android:id="@+id/describe_3"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/common_padding_default"
                        android:text="@string/des_Tl"
                        android:textSize="15sp"
                        app:layout_constraintStart_toStartOf="@id/title_tx1_3"
                        app:layout_constraintTop_toBottomOf="@id/title_tx1_3"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginBottom="10dp"
                        />


                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
    <include layout="@layout/activity_base" android:id="@+id/mn_barra_test"/>
</androidx.constraintlayout.widget.ConstraintLayout>