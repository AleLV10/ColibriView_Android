<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ishijara2">

    <!--HOLA HOLA-->


    <TextView
        android:id="@+id/menutest"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:text="@string/completado"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="@color/color"
        android:textSize="@dimen/tamaño_letra_grande"
        android:layout_marginTop="130dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <Button
        android:id="@+id/boton12"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginLeft="@dimen/margin3"
        android:text="@string/continuar"
        android:textSize="@dimen/tamaño_letra_titulo"
        android:layout_marginTop="@dimen/margin_top2"
        android:layout_marginRight="@dimen/margin3"
        android:backgroundTint="@color/moradoo"
        app:layout_constraintTop_toBottomOf="@id/menutest"
        />

    <Button
        android:id="@+id/boton22"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginLeft="@dimen/margin3"
        android:text="@string/resultados"
        android:textSize="@dimen/tamaño_letra_titulo"
        android:layout_marginTop="@dimen/margin4"
        android:layout_marginRight="@dimen/margin3"
        android:backgroundTint="@color/moradoo"
        app:layout_constraintTop_toBottomOf="@id/boton12"
        />

</androidx.constraintlayout.widget.ConstraintLayout>